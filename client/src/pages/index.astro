---
import DefaultLayout from "../layouts/default.astro";
import { sanityClient } from "sanity:client";
import PageBuilder from "../components/page-builder.astro";

const pageContent = await sanityClient.fetch(
  `*[_type == "page" && slug.current == 'homepage'][0]`
);
---

<DefaultLayout>
  <PageBuilder blocks={pageContent.content} />
</DefaultLayout>
